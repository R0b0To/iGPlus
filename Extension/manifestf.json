{
    "manifest_version":2,
    "name": "iGPlus",
    "description": "Enhance your iGPmanager experience",
    "version":"0.9962",
     "icons":{
         "128": "./images/Icon128.png"
     },
     "background": {
         "scripts": ["background.js"]
     },
     "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
      "browser_action": {
        "default_icon": "./images/Icon128.png",
        "default_title": "iGPlus",
        "default_popup": "popup.html"
      },
     "permissions":[
         "tabs",
         "storage",
         "scripting",
         "*://igpmanager.com/*",
         "*://api.open-meteo.com/*",
         "*://docs.google.com/*"
     ]

    
}