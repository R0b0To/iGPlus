{
  "name": "igplus",
  "version": "1.0.0",
  "description": "Extension for igpmanager game. Chrome and Firefox compatibility",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run:ff": "web-ext run --source-dir ./Extension",
    "lint:ff": "web-ext lint --source-dir ./Extension",
    "swapManifest": "./swapManifest.sh",
    "androidFirefox":"web-ext run -t firefox-android --android-device=localhost:57910 --firefox-apk org.mozilla.fenix --source-dir ./Extension",
    "swapManifest-win":"cd ./Extension && IF EXIST manifest-ff.json (REN manifest.json manifest-c.json & REN manifest-ff.json manifest.json) ELSE IF EXIST manifest-c.json (REN manifest.json manifest-ff.json & REN manifest-c.json manifest.json)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R0b0To/iGPlus.git"
  },
  "author": {
    "name": "R0b0To"
  },
  "contributors": [
    {
      "name": "evhenious",
      "email": "evhenious@gmail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/R0b0To/iGPlus/issues"
  },
  "homepage": "https://github.com/R0b0To/iGPlus#readme",
  "devDependencies": {
    "eslint": "^8.34.0",
    "web-ext": "^7.5.0"
  }
}
